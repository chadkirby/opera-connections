{
  "name": "operadle",
  "version": "0.0.0",
  "description": "guess operas",
  "repository": "https://github.com/chadkirby/operadle.git",
  "author": "Chad Kirby <chad.kirby@gmail.com>",
  "license": "UNLICENSED",
  "private": false,
  "type": "module",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "parcel",
    "build": "parcel build --target pages",
    "deploy": "git push origin :gh-pages && git subtree push --prefix gh-pages origin gh-pages"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jsdom": "^20.0.0",
    "node-fetch": "^3.2.9",
    "parcel": "^2.6.2",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "prettier": "^2.7.1",
    "serve-static": "^1.15.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@ckirby/block-re": "^3.0.1",
    "autocompleter": "^6.1.3",
    "canvas-gauges": "^2.1.7-radial",
    "compromise": "^14.4.2",
    "fastest-levenshtein": "1.0.13",
    "fuse.js": "^6.6.2",
    "seedrandom": "^3.0.5"
  },
  "targets": {
    "main": {
      "sourceMap": true,
      "includeNodeModules": [
        "fastest-levenshtein",
        "fuse.js",
        "canvas-gauges"
      ]
    },
    "pages": {
      "publicUrl": "/operadle",
      "distDir": "gh-pages",
      "includeNodeModules": [
        "fastest-levenshtein",
        "fuse.js",
        "canvas-gauges"
      ]
    }
  },
  "staticFiles": {
    "staticPath": "src/assets",
    "staticOutPath": "assets"
  }
}
